{
"update_url": "https://clients2.google.com/service/update2/crx",

	"name": "Web Scheduler",
	"version": "Beta",
	"description": "Scheduale and open websites automatically.",
	"permissions": ["storage", "background", "tabs"],
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},
	"options_page": "options.html",
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "images/auto_open_logo16.png",
			"32": "images/auto_open_logo32.png",
			"48": "images/auto_open_logo48.png",
			"128": "images/auto_open_logo128.png"
		}
	},
	"icons": {
		"16": "images/auto_open_logo16.png",
		"32": "images/auto_open_logo32.png",
		"48": "images/auto_open_logo48.png",
		"128": "images/auto_open_logo128.png"
	},
	"minimum_chrome_version": "79",
	"manifest_version": 2
}